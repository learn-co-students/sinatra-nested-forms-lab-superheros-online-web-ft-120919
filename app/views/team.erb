<h1>Create a Team and Heroes!</h1>
<form action="/teams" method="post">
    Team Name: <input type="text" name="team[name]">
    Team Motto: <input type="text" name="team[motto]">
    Hero 1 Name: <input type="text" id="member1_name" name="team[members][][name]">
    Hero 1 Power:<input type="text" id="member1_power" name="team[members][][power]">
    Hero 1 Biography: <input type="text" id="member1_bio" name="team[members][][bio]">
    Hero 2 Name: <input type="text" id="member2_name" name="team[members][][name]">
    Hero 2 Power:<input type="text" id="member2_power" name="team[members][][power]">
    Hero 2 Biography: <input type="text" id="member2_bio" name="team[members][][bio]">
    Hero 3 Name: <input type="text" id="member3_name" name="team[members][][name]">
    Hero 3 Power:<input type="text" id="member3_power" name="team[members][][power]">
    Hero 3 Biography: <input type="text" id="member3_bio" name="team[members][][bio]">
    <input type="submit" value="submit">
</form>



